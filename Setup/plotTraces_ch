#!/usr/bin/gnuplot
# This script is called by 'tra' script
# CH should be provided by external call
unset key
set title "Channel ".CH

tracesize = 8192
min = CH * tracesize
max = min + tracesize - 1

set xtics 0,tracesize/2
plot '/tmp/traces.dat' binary format="%ushort%ushort" every ::min::max with histeps